#include <stdio.h>

int main()
{
    int arr[10]={4,1,6,8,10,21,8,9,2,6};
    int max,min;
    max=0;
    min=100;
    for(int i=0;i<9;i++){
        if(arr[i]>max){
            max=arr[i];
        }
        if(arr[i]<min){
            min=arr[i];
        }
    }
    printf("%d",max);
    printf("\n %d",min);
    return 0;
    }
