#include <stdio.h>

void main() {
     char chr;
     printf("Enter a character");
     scanf("%c",&chr);
     if(chr='A' && chr='Z' || chr='a' && chr='z'){
         printf("Its an alphabet");
     }
     else if(chr=='0' && chr=='9'){
         printf("Its a digit");
     }
     else{
         printf("It is a special character");
     }
        
    
}
